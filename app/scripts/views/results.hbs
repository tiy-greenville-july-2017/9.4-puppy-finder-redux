<div class="row">
  <div class="col-md-12">

    <h1>{{name}}, we found {{dog.name}}!</h1>

    <div class="thumbnail puppy-{{dog.name}}">
      <img src="{{dog.pic}}" alt="{{dog.name}}">
      <div class="caption">
        <h3>{{dog.name}}</h3>
      </div>
    </div>

    <button type="button" name="button" class="btn btn-primary btn-lg text-center">Seach Again</button>

    <div class="jumbotron">
      <h2>Other Hip Dogs We Considered</h2>

      <div class="row">

        {{#each puppyList}}
          <div class="col-sm-6 col-md-4">
            <div class="thumbnail puppy-{{name}}">
              <img src="..." alt="...">
              <div class="caption">
                <h3>{{name}}</h3>
                <p>...</p>
                <p><a href="#" class="btn btn-primary js-puppy-select" role="button" data-puppy-id="{{@index}}">Select</a></p>
              </div>
            </div>
          </div>

        {{/each}}
      </div>

    </div>

  </div>
</div>
